{
    "manifest_version": 3,
    "description" : "drawing extention on any webpage.",
    "name":"p5 sketch",
    "version":"0.1",
   
    "permissions":["tabs"],
    "background":{
        "service_worker": "./background.js"
    },
    "content_scripts":[
        {
            "js":["p5/p5.js","sketch.js"],
            "matches":["<all_urls>"],
            "all_frames": false
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [ "./JSON/data.json" ],
            "matches": ["<all_urls>"]
        }
    ],
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "a popup"
    }   

}